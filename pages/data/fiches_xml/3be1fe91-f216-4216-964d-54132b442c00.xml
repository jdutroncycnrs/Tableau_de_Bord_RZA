            <gmd:CI_Citation>
               <gmd:title>
                  <gco:CharacterString>Base de données géographique de l'Observatoire des Sédiments du Rhône</gco:CharacterString>
               </gmd:title>
               <gmd:date>
                  <gmd:CI_Date>
                     <gmd:date>
                        <gco:Date>2020-09-23</gco:Date>
                     </gmd:date>
                     <gmd:dateType>
