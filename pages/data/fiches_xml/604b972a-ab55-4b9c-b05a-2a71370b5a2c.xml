               <gmd:title>
                  <gco:CharacterString>COMERE - Données d'âge de mort d'animaux domestiques archéologiques en Eurasie de l'ouest</gco:CharacterString>
               </gmd:title>
               <gmd:date>
                  <gmd:CI_Date>
                     <gmd:date>
                        <gco:Date>2020-04-14</gco:Date>
                     </gmd:date>
                     <gmd:dateType>
