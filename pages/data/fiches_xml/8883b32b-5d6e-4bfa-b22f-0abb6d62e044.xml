               <gmd:edition>
                  <gco:CharacterString>Création de données génériques pour le LabEx DRIIHM et l'OHMI Estarreja</gco:CharacterString>
               </gmd:edition>
               <gmd:identifier>
                  <gmd:MD_Identifier>
                     <gmd:code>
                        <gco:CharacterString>8883b32b-5d6e-4bfa-b22f-0abb6d62e044</gco:CharacterString>
