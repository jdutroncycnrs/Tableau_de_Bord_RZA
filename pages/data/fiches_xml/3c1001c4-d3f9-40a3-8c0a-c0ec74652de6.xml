            <gmd:CI_Citation>
               <gmd:title>
                  <gco:CharacterString>Base de données sur les grands singes : écologie, santé et comportement en milieu naturel</gco:CharacterString>
               </gmd:title>
               <gmd:date>
                  <gmd:CI_Date>
                     <gmd:date>
                        <gco:Date>2014-01-01</gco:Date>
                     </gmd:date>
                     <gmd:dateType>
