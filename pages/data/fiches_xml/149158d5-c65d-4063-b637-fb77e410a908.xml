            <gmd:CI_Citation>
               <gmd:title>
                  <gco:CharacterString>Valeurs de concentration en éléments chimiques dans des mousses collectées en France métropolitaine en 2011</gco:CharacterString>
               </gmd:title>
               <gmd:date>
                  <gmd:CI_Date>
                     <gmd:date>
                        <gco:Date>2023-03-03</gco:Date>
                     </gmd:date>
                     <gmd:dateType>
