tring": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Logements- Bassin-versant du littoral marseillais - 1968/2012"}}, "gmd:date": {"gmd:CI_Date": {"gmd:date": {"gco:DateTime": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "2016-05-10T12:00:00"}}, "gmd:dateType": {"gmd:CI_DateTypeCode": {"@codeListValue": "creation", "@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_DateTypeCode"}}}}, "gmd:edition": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}}}, "gmd:abstract": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Donn\u00e9es statistiques. Nombre de logements des communes du bassin-versant du littoral marseillais (36 communes) aux dates des recensements entre 1968 et 2012. La donn\u00e9e est d\u00e9taill\u00e9e par type de logement (nombre total de logements, r\u00e9sidences principales, r\u00e9sidences secondaires, logements vacants).\n\nClasses OHM - LM : Activit\u00e9 > Logements > Recensements\n\nDonn\u00e9e li\u00e9e au projet \"BEEST - Biomass Equivalency Ecosystem Services Transfer\" (2015-2018) financ\u00e9 par SUEZ Environnement et associant SUEZ aux laboratoires ESPACE et MIO.\nLe \"bassin-versant du littoral marseillais\" fait r\u00e9f\u00e9rence \u00e0 l'emprise g\u00e9ographique du projet. Les communes concern\u00e9es par ce bassin-versant sont totalement ou partiellement comprises dans celui-ci (36 communes au total dont 6 dans le d\u00e9partement du Var). Ce bassin-versant est d\u00e9fini dans la fiche de m\u00e9tadonn\u00e9e correspondante."}}, "gmd:purpose": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:status": {"gmd:MD_ProgressCode": {"@codeListValue": "completed", "@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#MD_ProgressCode"}}, "gmd:pointOfContact": {"@xmlns:gss": "http://www.isotc211.org/2005/gss", "gmd:CI_ResponsibleParty": {"gmd:individualName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Samuel ROBERT"}}, "gmd:organisationName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "ESPACE (UMR 7300)"}}, "gmd:positionName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Directeur de recherche CNRS"}}, "gmd:contactInfo": {"gmd:CI_Contact": {"gmd:phone": {"gmd:CI_Telephone": {"gmd:voice": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "04.13.94.94.21"}}, "gmd:facsimile": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}}}, "gmd:address": {"gmd:CI_Address": {"gmd:deliveryPoint": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Technopole de l'environnement Arbois M\u00e9diterran\u00e9e"}}, "gmd:city": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Aix-en-Provence"}}, "gmd:administrativeArea": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:postalCode": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "13545"}}, "gmd:country": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "France"}}, "gmd:electronicMailAddress": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "samuel.robert@univ-amu.fr"}}}}, "gmd:contactInstructions": {"gmx:FileName": {"@xmlns:gmx": "http://www.isotc211.org/2005/gmx", "@src": "http://datacatalog.ohm-littoral-mediterraneen.fr/geonetworklittmed/srv/fre/../../images/harvesting/Logo_UMR-ESPACE.jpg"}}}}, "gmd:role": {"gmd:CI_RoleCode": {"@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_RoleCode", "@codeListValue": "pointOfContact"}}}}, "gmd:resourceMaintenance": {"gmd:M