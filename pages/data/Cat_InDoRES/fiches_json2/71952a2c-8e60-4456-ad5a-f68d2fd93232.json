on": {"@xmlns:gfc": "http://www.isotc211.org/2005/gfc", "@xmlns:gmi": "http://standards.iso.org/iso/19115/-2/gmi/1.0", "@xmlns:gmlcov": "http://www.opengis.net/gmlcov/1.0", "@xmlns:gmlrgrid": "http://www.opengis.net/gml/3.3/rgrid", "gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "1.0"}}, "gmd:referenceSystemInfo": {"@xmlns:gfc": "http://www.isotc211.org/2005/gfc", "@xmlns:gmi": "http://standards.iso.org/iso/19115/-2/gmi/1.0", "@xmlns:gmlcov": "http://www.opengis.net/gmlcov/1.0", "@xmlns:gmlrgrid": "http://www.opengis.net/gml/3.3/rgrid", "gmd:MD_ReferenceSystem": {"gmd:referenceSystemIdentifier": {"gmd:RS_Identifier": {"gmd:code": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "4326"}}, "gmd:codeSpace": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "EPSG"}}}}}}, "gmd:identificationInfo": {"@xmlns:gfc": "http://www.isotc211.org/2005/gfc", "@xmlns:gmi": "http://standards.iso.org/iso/19115/-2/gmi/1.0", "@xmlns:gmlcov": "http://www.opengis.net/gmlcov/1.0", "@xmlns:gmlrgrid": "http://www.opengis.net/gml/3.3/rgrid", "gmd:MD_DataIdentification": {"gmd:citation": {"gmd:CI_Citation": {"gmd:title": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "La R\u00e9union, Carte d'occupation du sol 2018"}}, "gmd:date": {"gmd:CI_Date": {"gmd:date": {"gco:DateTime": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "2022-05-17T19:09:33"}}, "gmd:dateType": {"gmd:CI_DateTypeCode": {"@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_DateTypeCode", "@codeListValue": "revision", "@codeSpace": "ISOTC211/19115"}}}}, "gmd:edition": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "1.0"}}, "gmd:editionDate": {"gco:Date": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "2015-01-01"}}, "gmd:identifier": {"gmd:MD_Identifier": {"gmd:code": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "http://dx.doi.org/doi:10.18167/DVN1/SH9CWG"}}}}, "gmd:citedResponsibleParty": {"gmd:CI_ResponsibleParty": {"gmd:individualName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Stephane Dupuy"}}, "gmd:organisationName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "CIRAD"}}, "gmd:contactInfo": {"gmd:CI_Contact": {"gmd:address": {"gmd:CI_Address": {"gmd:electronicMailAddress": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "stephane.dupuy@cirad.fr"}}}}}}, "gmd:role": {"gmd:CI_RoleCode": {"@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_RoleCode", "@codeListValue": "principalInvestigator", "@codeSpace": "ISOTC211/19115"}}}}}}, "gmd:abstract": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Cette carte est issue de travaux de recherche men\u00e9s dans le cadre du projet GABIR (Gestion Agricole des Biomasses \u00e0 l\u2019\u00e9chelle de l\u2019Ile de la R\u00e9union). Elle a \u00e9t\u00e9 produite en utilisant une mosa\u00efque d'images Spot6/7 pour calculer la segmentation (extraction d'objets homog\u00e8nes \u00e0 partir de l'image). Nous utilisons une base de donn\u00e9es terrain ayant une nomenclature emboit\u00e9e avec 3 niveaux de pr\u00e9cision nous permettant de produire une classification par niveau. Le niveau 3, le plus d\u00e9taill\u00e9 distinguant les types de cultures pr\u00e9sente une pr\u00e9cision globale de 86% et un indice de Kappa est de 0,85. Le niveau 2, distinguant les groupes de cultures pr\u00e9sente une pr\u00e9cision globale de 91% et un indice de Kappa est de 0,90. Le niveau 1, distinguant les grands groupes d'occupation du sol pr\u00e9sente une pr\u00e9cision globale de 97% et un indice de Kappa est de 0,95. (2019-06-12) La pr\u00e9cision globale de la carte est de 88,71 % et l'indice de Kappa est de 86,47%. La chaine Moringa est mise au point au sein du CES Occupation des Sols du p\u00f4le THEIA pour cartographier l\u2019occupation du sol dans les contextes vari\u00e9s des pays du Sud. Afin d\u2019\u00eatre plus facilement reproductible, elle est automatis\u00e9e et son impl\u00e9mentation est r\u00e9alis\u00e9e avec des outils libres (Orfeo Toolbox, R, Python). (2019-03-26)"}}, "gmd:status": {"gmd:MD_ProgressCode": {"@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#MD_ProgressCode", "@codeListValue": "completed"}}, "gmd:pointOfContact": {"gmd:CI_ResponsibleParty": {"gmd:individualName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Stephane Dupuy"}}, "gmd:organisationName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "CIRAD"}}, "gmd:contactInfo": {"gmd:CI_Contact": {"gmd:address": {"gmd:CI_Address": {"gmd:electronicMailAddress": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "stephane.dupuy@cirad.fr"}}}}}}, "gmd:role": {"gmd:CI_RoleCode": {"@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_RoleCode", "@codeListValue": "principalInvestigator", "@codeSpace": "ISOTC211/19115"}}}}, "gmd:resourceMaintenance": {"gmd:MD_MaintenanceInformation": {"gmd:maintenanceAndUpdateFrequency": {"gmd:MD_MaintenanceFrequencyCode": {"@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#MD_MaintenanceFrequencyCode", "@codeListValue": "asNeeded", "@codeSpace": "ISOTC211/19115"}}}}, "gmd:graphicOverview": {"gmd:MD_BrowseGraphic": {"gmd:fileName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "http://idg-tetis.teledetection.fr//data/vignettes/CarteOccSolReunion2018.png"}}, "gmd:fileDescription": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Carte Occupation du Sol"}}, "gmd:fileType": {"gco:CharacterString": {"@xmlns:gco": "http://www.i