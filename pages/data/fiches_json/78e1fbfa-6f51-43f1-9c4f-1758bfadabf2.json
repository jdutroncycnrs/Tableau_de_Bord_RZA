                        "gco:CharacterString": {
                            "@xmlns:gco": "http://www.isotc211.org/2005/gco",
                            "#text": "S\u00e9rie temporelles des concentrations de Hg, Cd et Pb chez le dauphin et marsouin commun"
                        }
                    },
                    "gmd:date": {
                        "gmd:CI_Date": {
