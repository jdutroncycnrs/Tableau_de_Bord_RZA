            "gmd:citation": {
                "gmd:CI_Citation": {
                    "gmd:title": {
                        "gco:CharacterString": {
                            "@xmlns:gco": "http://www.isotc211.org/2005/gco",
                            "#text": "Valeurs de concentration en \u00e9l\u00e9ments chimiques dans des mousses collect\u00e9es en France m\u00e9tropolitaine en 2006"
                        }
                    },
                    "gmd:date": {
                        "gmd:CI_Date": {
                            "gmd:date": {
                                "gco:Date": {
                                    "@xmlns:gco": "http://www.isotc211.org/2005/gco",
