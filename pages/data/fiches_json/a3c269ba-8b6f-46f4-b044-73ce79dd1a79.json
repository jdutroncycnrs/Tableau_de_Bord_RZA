                    "gmd:title": {
                        "gco:CharacterString": {
                            "@xmlns:gco": "http://www.isotc211.org/2005/gco",
                            "#text": "\"Cyber-caroth\u00e8que nationale\" C2FN : inventaire national des carottages et carottes sedimentaires scientifiques port\u00e9s par des laboratoires de recherche francais"
                        }
                    },
                    "gmd:date": {
                        "gmd:CI_Date": {
                            "gmd:date": {
                                "gco:DateTime": {
                                    "@xmlns:gco": "http://www.isotc211.org/2005/gco",
