                    "gmd:title": {
                        "gco:CharacterString": {
                            "@xmlns:gco": "http://www.isotc211.org/2005/gco",
                            "#text": "Observation de la renou\u00e9e dans les Alpes du Nord"
                        }
                    },
                    "gmd:date": {
                        "gmd:CI_Date": {
                            "gmd:date": {
                                "gco:Date": null
                            },
                            "gmd:dateType": {
