                    "gmd:title": {
                        "gco:CharacterString": {
                            "@xmlns:gco": "http://www.isotc211.org/2005/gco",
                            "#text": "Data from the article: Experimental evidence of early costs of reproduction in conspecific viviparous and oviparous lizards"
                        }
                    },
                    "gmd:date": {
                        "gmd:CI_Date": {
                            "gmd:date": {
