dress": {"gmd:deliveryPoint": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:city": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:administrativeArea": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:postalCode": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:country": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:electronicMailAddress": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}}}}}, "gmd:role": {"gmd:CI_RoleCode": {"@codeListValue": "pointOfContact", "@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_RoleCode"}}}}, "gmd:dateStamp": {"@xmlns:gml": "http://www.opengis.net/gml", "gco:DateTime": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "2022-12-06T13:44:34.754Z"}}, "gmd:metadataStandardName": {"@xmlns:gml": "http://www.opengis.net/gml", "gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "ISO 19115:2003/19139"}}, "gmd:metadataStandardVersion": {"@xmlns:gml": "http://www.opengis.net/gml", "gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "1.0"}}, "gmd:referenceSystemInfo": {"@xmlns:gml": "http://www.opengis.net/gml", "gmd:MD_ReferenceSystem": {"gmd:referenceSystemIdentifier": {"gmd:RS_Identifier": {"gmd:code": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "WGS 1984"}}}}}}, "gmd:identificationInfo": {"@xmlns:gml": "http://www.opengis.net/gml", "gmd:MD_DataIdentification": {"gmd:citation": {"gmd:CI_Citation": {"gmd:title": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Liste des archives d\u00e9pouill\u00e9es 2017-2018"}}, "gmd:date": {"gmd:CI_Date": {"gmd:date": {"gco:DateTime": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "2019-10-10"}}, "gmd:dateType": {"gmd:CI_DateTypeCode": {"@codeListValue": "publication", "@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_DateTypeCode"}}}}, "gmd:edition": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}}}, "gmd:abstract": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Liste des archives d\u00e9pouill\u00e9es lors du projet de recherche OHM 2017 \u00ab Le r\u00f4le des responsables politiques locaux dans l\u2019\u00e9volution des relations entre l\u2019usine d\u2019alumine de Gardanne et son milieu, XIXe-XXe. \u00bb L\u2019objet de l\u2019\u00e9tude historique men\u00e9e fut de proposer une entr\u00e9e politique dans le sujet, \u00e0 savoir comment l\u2019usine et ses nuisances, notamment li\u00e9es \u00e0 l\u2019environnement, ont \u00e9t\u00e9 prises en consid\u00e9ration par les responsables politiques locaux au fil du XXe si\u00e8cle \u2013 si elles l\u2019ont \u00e9t\u00e9 - comment ce groupe d\u2019acteurs a interagi, voire influenc\u00e9 les autres groupes d\u2019acteurs concern\u00e9s par l\u2019activit\u00e9 de l\u2019usine et ainsi particip\u00e9 \u00e0 l\u2019\u00e9volution de l\u2019acceptabilit\u00e9 sociale du site industriel. Il s\u2019agit de la premi\u00e8re \u00e9tape d\u2019un projet de recherche sur le long terme visant \u00e0 produire une analyse historique sur l\u2019histoire de l\u2019industrie de l\u2019alumine et de son impact sur l\u2019environnement, sur les relations entre les usines de production d\u2019alumine et leur milieu en Europe (programme de recherche international en cours de constitution)"}}, "gmd:purpose": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:status": {"gmd