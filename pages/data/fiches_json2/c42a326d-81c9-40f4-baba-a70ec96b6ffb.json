: null}}}, "gmd:address": {"gmd:CI_Address": {"gmd:deliveryPoint": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:city": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:administrativeArea": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:postalCode": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:country": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:electronicMailAddress": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "bdutrill@mnhn.fr"}}}}}}, "gmd:role": {"gmd:CI_RoleCode": {"@codeListValue": "pointOfContact", "@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_RoleCode"}}}}, "gmd:dateStamp": {"@xmlns:geonet": "http://www.fao.org/geonetwork", "gco:DateTime": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "2023-11-23T09:10:13.222Z"}}, "gmd:metadataStandardName": {"@xmlns:geonet": "http://www.fao.org/geonetwork", "gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "ISO 19115:2003/19139"}}, "gmd:metadataStandardVersion": {"@xmlns:geonet": "http://www.fao.org/geonetwork", "gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "1.0"}}, "gmd:referenceSystemInfo": {"@xmlns:geonet": "http://www.fao.org/geonetwork", "gmd:MD_ReferenceSystem": {"gmd:referenceSystemIdentifier": {"gmd:RS_Identifier": {"gmd:code": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "WGS 1984"}}}}}}, "gmd:identificationInfo": {"@xmlns:geonet": "http://www.fao.org/geonetwork", "gmd:MD_DataIdentification": {"gmd:citation": {"gmd:CI_Citation": {"gmd:title": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Caryotypes, images de m\u00e9iose et photos d'insectes Col\u00e9opt\u00e8res du monde"}}, "gmd:date": {"gmd:CI_Date": {"gmd:date": {"gco:Date": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "2018-01-01"}}, "gmd:dateType": {"gmd:CI_DateTypeCode": {"@codeListValue": "creation", "@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_DateTypeCode"}}}}}}, "gmd:abstract": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Ces donn\u00e9es se rapportent \u00e0 des informations sur la cytog\u00e9n\u00e9tique (\u00e9tude des chromosomes) de plusieurs centaines d'esp\u00e8ces de Col\u00e9opt\u00e8res. Environ la moiti\u00e9 ont d\u00e9j\u00e0 fait l'objet de publications dans la litt\u00e9rature internationale sous une forme restreinte, et les r\u00e9f\u00e9rences sont alors indiqu\u00e9es. Pour les quelques 200 autres esp\u00e8ces, les donn\u00e9es sont enti\u00e8rement originales. Pour chaque esp\u00e8ce, un jeu d'un minimum de 2 photographies est pr\u00e9sent\u00e9, \u00e0 savoir : une photo de l'insecte permettant de l'identifier et un caryotype apr\u00e8s coloration par le Giemsa. Dans la tr\u00e8s grande majorit\u00e9 des cas, 3 autres photos repr\u00e9sentent le caryotype apr\u00e8s marquage C (de l'h\u00e9t\u00e9rochromatine costitutive) et des images de division m\u00e9iotique \u00e0 diff\u00e9rents stades."}}, "gmd:status": {"gmd:MD_ProgressCode": {"@codeListValue": "onGoing", "@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#MD_ProgressCode"}}, "gmd:pointOfContact": [{"gmd:CI_ResponsibleParty": {"gmd:individualName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "DUTRILLAUX Bernard"}}, "gmd:organisationName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Mus\u00e9um national d'Histoire naturelle"}}, "gmd:positionName": {"@xmlns:gco": "http://www.isotc