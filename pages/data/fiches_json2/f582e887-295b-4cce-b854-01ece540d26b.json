etadataStandardName": {"@xmlns:gml": "http://www.opengis.net/gml", "gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "ISO 19115:2003/19139"}}, "gmd:metadataStandardVersion": {"@xmlns:gml": "http://www.opengis.net/gml", "gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "1.0"}}, "gmd:dataSetURI": {"@xmlns:gml": "http://www.opengis.net/gml", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:referenceSystemInfo": {"@xmlns:gml": "http://www.opengis.net/gml", "#text": ["\n    \"\n    ", "\n    \"\n  "], "gmd:MD_ReferenceSystem": {"#text": ["\n      \"\n      ", "\n      \"\n    "], "gmd:referenceSystemIdentifier": {"#text": ["\n        \"\n        ", "\n        \"\n      "], "gmd:RS_Identifier": {"gmd:code": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "REFERENTIEL DE COORDONNEES (WGS 84, LAMBERT 93, ETC)"}}}}}}, "gmd:identificationInfo": {"@xmlns:gml": "http://www.opengis.net/gml", "gmd:MD_DataIdentification": {"gmd:citation": {"#text": ["\n        \"\n        ", "\n        \"\n      "], "gmd:CI_Citation": {"gmd:title": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Infiltration campaign in the Mercier sub-catchment (Yzeron catchment, periphery of Lyon France) in 2008"}}, "gmd:date": [{"#text": ["\n            \"\n            ", "\n            \"\n          "], "gmd:CI_Date": {"#text": ["\n              \"\n              ", "\n              \"\n              ", "\n              \"\n            "], "gmd:date": {"gco:DateTime": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "2008-10-15T12:00:00"}}, "gmd:dateType": {"#text": ["\n                \"\n                ", "\n                \"\n              "], "gmd:CI_DateTypeCode": {"@codeListValue": "creation", "@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_DateTypeCode"}}}}, {"#text": ["\n            \"\n            ", "\n            \"\n          "], "gmd:CI_Date": {"#text": ["\n              \"\n              ", "\n              \"\n              ", "\n              \"\n            "], "gmd:date": {"gco:DateTime": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "2009-05-15T12:00:00"}}, "gmd:dateType": {"#text": ["\n                \"\n                ", "\n                \"\n              "], "gmd:CI_DateTypeCode": {"@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_DateTypeCode", "@codeListValue": "publication"}}}}], "gmd:editionDate": {"gco:DateTime": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "1914-07-26T14:30:00"}}}}, "gmd:abstract": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "This study was part of the AVuPUR project (Assessing the Vulnerability of PeriUrban catchments, 2008-2011), dedicated to the observation and understanding of the impact of urbanization on the hydrological cycle in periurban catchments. We provide the results of a field campaign conducted in the Mercier catchment in 2008-2009, a sub-catchment of the Yzeron catchment located in the Monts du Lyonnais, in the western part of Lyon city, France. The campaign was dedicated to the characterization of top soil hydraulic properties. Some data are also available at -15 and -30 cm depth. \nThe data set includes 66 sampled points with information of particle size data, dry bulk density and porosity, hydraulic conductivity at saturation and close to saturation for 46 measurement points. \nWe also provide estimates of the parameters of the retention and hydraulic conductivity curves for the various sample points.\nSample points were selected in the Mercier catchment (6.7 km2)by combining information about geology, land use and pedology. Two types of infiltration experiments were conducted: single ring infiltration tests with a positive pressure at the soil surface (3 replicates)(Beerkan method)_ and mini-disk infiltrometers using a -20 mm suction (0 to 3 replicates).\nThe data can be downloaded at  https://doi.org/10.5281/zenodo.3352164"}}, "gmd:status": {"#text": ["\n        \"\n        ", "\n        \"\n      "], "gmd:MD_ProgressCode": {"@codeListValue": "completed", "@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#MD_ProgressCode"}}, "gmd:pointOfContact": {"gmd:CI_ResponsibleParty": {"gmd:individualName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Isabelle BRAUD"}}, "gmd:organisationName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "IRSTEA"}}, "gmd:positionName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Directrice de recherches"}}, "gmd:contactInfo": {"gmd:CI_Contact": {"gmd:phone": {"gmd:CI_Telephone": {"gmd:voice": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "04 72 20 87 78"}}}}, "gmd:address": {"gmd:CI_Address": {"gmd:deliveryPoint": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "5 rue de la Doua"}}, "gmd:city": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "VILLEURBANNE"}}, "gmd:administrativeArea": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "@gco:nilReason": "missing", "gco:CharacterString": null}, "gmd:postalCode": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "696026"}}, "gmd:country": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "France"}}, "gmd:electronicMailAddress": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "isabelle.braud@irstea.fr"}}}}, "gmd:onlineResource": {"gmd:CI_OnlineResource": {"gmd:linkage": {"gmd:URL": "http://www.irstea.fr/accueil"}, "gmd:protocol": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "WWW:LINK-1.0-http--link"}}, "gmd:name":