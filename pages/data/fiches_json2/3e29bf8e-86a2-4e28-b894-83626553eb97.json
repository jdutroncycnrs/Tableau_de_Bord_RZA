mlns:gfc": "http://www.isotc211.org/2005/gfc", "@xmlns:gmi": "http://standards.iso.org/iso/19115/-2/gmi/1.0", "@xmlns:gmlcov": "http://www.opengis.net/gmlcov/1.0", "@xmlns:gmlrgrid": "http://www.opengis.net/gml/3.3/rgrid", "gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "1.0"}}, "gmd:referenceSystemInfo": {"@xmlns:gfc": "http://www.isotc211.org/2005/gfc", "@xmlns:gmi": "http://standards.iso.org/iso/19115/-2/gmi/1.0", "@xmlns:gmlcov": "http://www.opengis.net/gmlcov/1.0", "@xmlns:gmlrgrid": "http://www.opengis.net/gml/3.3/rgrid", "gmd:MD_ReferenceSystem": {"gmd:referenceSystemIdentifier": {"gmd:RS_Identifier": {"gmd:code": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "4326"}}, "gmd:codeSpace": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "EPSG"}}}}}}, "gmd:identificationInfo": {"@xmlns:gfc": "http://www.isotc211.org/2005/gfc", "@xmlns:gmi": "http://standards.iso.org/iso/19115/-2/gmi/1.0", "@xmlns:gmlcov": "http://www.opengis.net/gmlcov/1.0", "@xmlns:gmlrgrid": "http://www.opengis.net/gml/3.3/rgrid", "gmd:MD_DataIdentification": {"gmd:citation": {"gmd:CI_Citation": {"gmd:title": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "La R\u00e9union, Carte d'occupation du sol 2019"}}, "gmd:date": {"gmd:CI_Date": {"gmd:date": {"gco:DateTime": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "2022-05-17T19:09:42"}}, "gmd:dateType": {"gmd:CI_DateTypeCode": {"@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_DateTypeCode", "@codeListValue": "revision", "@codeSpace": "ISOTC211/19115"}}}}, "gmd:edition": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "1.0"}}, "gmd:editionDate": {"gco:Date": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "2015-01-01"}}, "gmd:identifier": {"gmd:MD_Identifier": {"gmd:code": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "3e29bf8e-86a2-4e28-b894-83626553eb97"}}}}, "gmd:citedResponsibleParty": {"gmd:CI_ResponsibleParty": {"gmd:individualName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Stephane Dupuy"}}, "gmd:organisationName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "CIRAD"}}, "gmd:contactInfo": {"gmd:CI_Contact": {"gmd:address": {"gmd:CI_Address": {"gmd:electronicMailAddress": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Stephane.Dupuy@cirad.fr"}}}}}}, "gmd:role": {"gmd:CI_RoleCode": {"@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_RoleCode", "@codeListValue": "principalInvestigator", "@codeSpace": "ISOTC211/19115"}}}}}}, "gmd:abstract": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Ce jeu de donn\u00e9es concerne les cartes produites, pour l'ann\u00e9e 2019, en utilisant une mosa\u00efque d'images Spot6/7 pour calculer la segmentation (extraction d'objets homog\u00e8nes \u00e0 partir de l'image). Nous utilisons une base de donn\u00e9es terrain ayant une nomenclature emboit\u00e9e avec 3 niveaux de pr\u00e9cision nous permettant de produire une classification par niveau. Le niveau le plus d\u00e9taill\u00e9 distinguant les types de cultures pr\u00e9sente une pr\u00e9cision globale de 88% et un indice de Kappa est de 0,86. Le niveau 2, distinguant les groupes de cultures pr\u00e9sente une pr\u00e9cision globale de 92% et un indice de Kappa est de 0,93. Le niveau 1, distinguant les grands groupes d'occupation du sol pr\u00e9sente une pr\u00e9cision globale de 97% et un indice de Kappa est de 0,95. Une fiche d\u00e9taill\u00e9e pr\u00e9sentant la m\u00e9thode et les r\u00e9sultats de validation est t\u00e9l\u00e9chargeable (2020-04-30)"}}, "gmd:purpose": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Cette carte est produite comme appui aux politiques publiques."}}, "gmd:status": {"gmd:MD_ProgressCode": {"@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#MD_ProgressCode", "@codeListValue": "completed"}}, "gmd:pointOfContact": {"gmd:CI_ResponsibleParty": {"gmd:individualName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Stephane Dupuy"}}, "gmd:organisationName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "CIRAD"}}, "gmd:contactInfo": {"gmd:CI_Contact": {"gmd:address": {"gmd:CI_Address": {"gmd:electronicMailAddress": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Stephane.Dupuy@cirad.fr"}}}}}}, "gmd:role": {"gmd:CI_RoleCode": {"@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#CI_RoleCode", "@codeListValue": "principalInvestigator", "@codeSpace": "ISOTC211/19115"}}}}, "gmd:resourceMaintenance": {"gmd:MD_MaintenanceInformation": {"gmd:maintenanceAndUpdateFrequency": {"gmd:MD_MaintenanceFrequencyCode": {"@codeList": "http://standards.iso.org/iso/19139/resources/gmxCodelists.xml#MD_MaintenanceFrequencyCode", "@codeListValue": "annually", "@codeSpace": "ISOTC211/19115"}}}}, "gmd:graphicOverview": {"gmd:MD_BrowseGraphic": {"gmd:fileName": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "http://idg-tetis.teledetection.fr/data/vignettes/CarteOccSolReunion2018.png"}}, "gmd:fileDescription": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Carte Occ Sol Reunion"}}, "gmd:fileType": {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "image/png"}}}}, "gmd:descriptiveKeywords": [{"gmd:MD_Keywords": {"gmd:keyword": [{"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "remote sensing"}}, {"gco:CharacterString": {"@xmlns:gco": "http://www.isotc211.org/2005/gco", "#text": "Agricultural Sciences